{% extends "base-page-2020.twig" %}

{% block content %}
	<div class="page-2020-wrap one-column">
        <article id="post-{{ post.id }}" {{ fn('post_class','fullwidth fullheight cf') }} role="article" itemscope itemtype="http://schema.org/BlogPosting">
            <div class="col-xs-24">
                <h1 class="projects-intro-headline">
                    {{ post.post_title }}
                </h1>
            </div>
            <div class="col-xs-24">
                {{ post.content | shortcodes }}
            </div>
        </article>
	</div>
{% endblock %}
